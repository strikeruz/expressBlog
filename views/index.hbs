{{#each posts}}
	<div class='col-lg-8 col-md-10 mx-auto'>
		<div class='post-preview'>
			<a href='/category/post/{{this.slug}}'>
				<h2 class='post-title'>
					{{this.title}}
				</h2>
				<h3 class='post-subtitle'>
					{{this.content}}
				</h3>
			</a>
			<p class='post-meta'>Posted by
				<a href='/category/post/{{this.slug}}'>Start Bootstrap</a>
				on September 24, 2019</p>
		</div>
		<hr />
	</div>
	<hr />
{{/each}}
